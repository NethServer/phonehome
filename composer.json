{
    "name": "tbaile/phonehome",
    "type": "project",
    "description": "Rewrite of old NethServer/nethserver-phonehome.",
    "keywords": ["nethesis", "ns", "nethserver", "phonehome", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "guzzlehttp/guzzle": "7.5.0",
        "laravel/framework": "9.35.1",
        "laravel/sanctum": "3.0.1",
        "laravel/tinker": "2.7.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.12.3",
        "brianium/paratest": "6.6.4",
        "fakerphp/faker": "1.20.0",
        "laravel/pint": "1.2.0",
        "laravel/sail": "1.16.2",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "6.3.1",
        "pestphp/pest": "1.22.1",
        "pestphp/pest-plugin-laravel": "1.3.0",
        "pestphp/pest-plugin-parallel": "1.2.0",
        "phpunit/phpunit": "9.5.25",
        "spatie/laravel-ignition": "1.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
