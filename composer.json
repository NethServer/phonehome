{
    "name": "nethserver/phonehome-server",
    "type": "project",
    "description": "Rewrite of old NethServer/nethserver-phonehome.",
    "keywords": [
        "nethesis",
        "ns",
        "nethserver",
        "phonehome",
        "laravel"
    ],
    "version": "1.0.0",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^8.3",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "geoip2/geoip2": "^v2.13",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^v10.21",
        "laravel/sanctum": "^v3.2",
        "laravel/tinker": "^v2.8",
        "league/csv": "^9.10",
        "opis/json-schema": "^2.3",
        "tronovav/geoip2-update": "^v2.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^v2.13",
        "fakerphp/faker": "^v1.23",
        "laravel/pint": "^v1.12",
        "laravel/sail": "^v1.24",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^v2.16",
        "pestphp/pest-plugin-laravel": "^v2.2",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
