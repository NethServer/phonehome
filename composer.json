{
    "name": "nethserver/phonehome-server",
    "type": "project",
    "description": "Rewrite of old NethServer/nethserver-phonehome.",
    "keywords": [
        "nethesis",
        "ns",
        "nethserver",
        "phonehome",
        "laravel"
    ],
    "version": "0.3.1",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^8.1",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "geoip2/geoip2": "v2.13.0",
        "guzzlehttp/guzzle": "7.5.0",
        "laravel/framework": "v9.44.0",
        "laravel/sanctum": "v3.0.1",
        "laravel/tinker": "v2.7.3",
        "league/csv": "9.8.0",
        "opis/json-schema": "2.3.0",
        "tronovav/geoip2-update": "v2.1.23"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "v2.12.3",
        "brianium/paratest": "v6.7.0",
        "fakerphp/faker": "v1.21.0",
        "laravel/pint": "v1.2.1",
        "laravel/sail": "v1.16.5",
        "marcocesarato/php-conventional-changelog": "1.16.0",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "v6.3.1",
        "nunomaduro/larastan": "2.2.9",
        "pestphp/pest": "v1.22.3",
        "pestphp/pest-plugin-laravel": "v1.3.0",
        "pestphp/pest-plugin-parallel": "v1.2.0",
        "phpunit/phpunit": "9.5.27",
        "spatie/laravel-ignition": "1.6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
